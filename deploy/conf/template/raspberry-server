upstream django {
    server unix:///var/run/uwsgi/uwsgi.sock;
}

server {
    listen      80;
    server_name raspberry-server.local;
    charset     utf-8;
    access_log  /home/pi-srv/app/RaspberryServer/pi/logs/access.log;
    error_log   /home/pi-srv/app/RaspberryServer/pi/logs/error.log; 

    location /static {
        root /home/pi-srv/app/RaspberryServer/pi/collectstatic;
    }

    location / {
        uwsgi_pass django;
        include /home/pi-srv/app/RaspberryServer/deploy/conf/template/uwsgi_params;
    }
}